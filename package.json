{
  "name": "eslint-plugin-sbtsbol",
  "version": "1.0.0",
  "author": "JSC SberTech",
  "description": "SBTSBOL specific linting rules for ESLint",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
