    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbtsbol/eslint-plugin-sbtsbol.git"
  },
  "keywords": [
    "styleguide",
    "javascript",
    "sbertech",
    "sbtsbol",
    "plugin",
    "eslintplugin",
    "eslint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sbtsbol/eslint-plugin-sbtsbol/issues"
  },
  "homepage": "https://github.com/sbtsbol/eslint-plugin-sbtsbol#readme",
  "dependencies": {
    "has": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-plugin-react": "~7.4.0",
    "istanbul": "0.4.5",
    "mocha": "^3.5.0"
  },
  "peerDependencies": {
    "eslint": "^3.0.0 || ^4.0.0"
  },
  "publishConfig": {
    "registry": "http://nexus.sigma.sbrf.ru:8099/nexus/content/repositories/sbol_npm_release/",
